{
	"name": "url-shortening-server",
	"version": "1.0.0",
	"description": "",
	"main": "index.js",
	"scripts": {
		"build": "npm run clean && tsc -p tsconfig.dist.json && npm run copy-files",
		"start": "tsc -w & nodemon index.js",
		"clean": "rm -rf dist",
		"lint": "eslint . --ext .ts --fix",
		"test": "jest",
		"copy-files": "cp src/schema/*.graphql dist/src/schema",
		"migrate": "npx sequelize-cli db:migrate:undo",
		"migrate:undo": "npx sequelize-cli db:migrate:undo",
		"seed": "npx sequelize-cli db:seed:all"
	},
	"author": "",
	"license": "ISC",
	"devDependencies": {
		"@types/jest": "^26.0.21",
		"@types/node": "^14.14.35",
		"@typescript-eslint/eslint-plugin": "^4.18.0",
		"@typescript-eslint/parser": "^4.18.0",
		"eslint": "^7.22.0",
		"graphql-tools": "^4.0.8",
		"jest": "^26.6.3",
		"nodemon": "^2.0.7",
		"sequelize-cli": "^6.2.0",
		"supertest": "^6.1.3",
		"ts-jest": "^26.5.4",
		"ts-node": "^9.1.1",
		"typescript": "^4.2.3"
	},
	"dependencies": {
		"apollo-server-express": "^2.21.2",
		"bcrypt": "^5.0.1",
		"dotenv": "^8.2.0",
		"express-graphql": "^0.12.0",
